@model LivestreamTest.Models.SpotifyCurrentlyPlayingModel

@if (Model != null)
{
    <div class="row">
        <div class="col-12">
            Currently Playing
        </div>
    </div>
    <div class="row">
        <div class="col-3">
            <img src="@Model.item.album.images.LastOrDefault().url" class="img-fluid" />
        </div>
        <div class="col-9">
            <div class="row">
                <div class="col-12">
                    @Model.item.name
                </div>
            </div>
            <div class="row">
                <div class="col-12">
                    @Model.item.artists.FirstOrDefault().name
                </div>
            </div>
        </div>
    </div>
}